seperator_block_width=15
markup=pango

[weather]
command="${HOME}/.config/i3blocks/blocks/weather"
interval=1800

[volume]
command="${HOME}/.config/i3blocks/blocks/volume"
signal=1
interval=once

[brightness]
command=light | cut -d"." -f1 | awk '{print $0 "%"}'
interval=once
signal=10
label=ðŸ”†

[battery]
command="${HOME}/.config/i3blocks/blocks/battery" BAT0
interval=10

[clock]
command=date '+%b %d (%a) %I:%M%p'
interval=10

[ssid]
command=iwgetid -r
interval=10

[help]
interval=once
