# cd back to the top of the git repo youre in

cdp() {
	local TEMP_PWD="$PWD"
	while [[ ! -d ./.git ]]; do
		cd ..
		# if we werent in a git repo, and we hit root
		if [[ $PWD == / ]]; then
			echo "Could not find a git repo in the current tree..." >&2
			cd "$TEMP_PWD"
			break
		fi
	done
	OLDPWD="$TEMP_PWD"
}
